{
    "version": 2,
    "builds": [
      {
        "src": "buildServer/index.js",
        "use": "@vercel/node",
        "config": { "includeFiles": ["buildServer/**"] }
      },
      {
        "src": "build/**",
        "use": "@vercel/static",
        "config": { "includeFiles": ["build/**"] }
      }
    ],
    "routes": [
      {
        "src": "/images",
        "dest": "buildServer/index.js"
      },
      {
        "src": "/products",
        "dest": "buildServer/index.js"
      },
      {
        "src": "/",
        "dest": "build/index.html"
      }
    ]
  }